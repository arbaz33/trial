
echo -n "enter the name of the command"
read name
case $name in
Date) 
     date
     ;;
Cal)
     cal
     ;;
Ls)
	ls
	;;
Pwd)
	pwd
	;;
Exit)
	exit
	;;
*)
	echo "Unknown command "
	exit
	;;
	esac
	
